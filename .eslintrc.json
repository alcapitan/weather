{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard", "eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "warn",
        "no-unused-vars": "off",
        "no-undef": "off",
        "prefer-const": "warn"
    }
}
